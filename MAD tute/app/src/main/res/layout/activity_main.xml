<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#fafaf6"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:backgroundTint="#fafaf6"
        tools:context=".MainActivity"
        tools:layout_editor_absoluteX="201dp"
        tools:layout_editor_absoluteY="-16dp">

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textSize="24dp"
            tools:layout_editor_absoluteX="21dp"
            tools:layout_editor_absoluteY="30dp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="102dp"
            android:layout_height="35dp"
            android:text="TextView"
            android:textSize="24dp"
            tools:layout_editor_absoluteX="21dp"
            tools:layout_editor_absoluteY="155dp" />

        <EditText
            android:id="@+id/editTextNumberDecimal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="numberDecimal"
            android:scrollbarSize="34dp"
            android:text="23"
            tools:layout_editor_absoluteX="21dp"
            tools:layout_editor_absoluteY="78dp" />

        <EditText
            android:id="@+id/editTextNumberDecimal2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="numberDecimal"
            android:scrollbarSize="34dp"
            android:text="12"
            tools:layout_editor_absoluteX="21dp"
            tools:layout_editor_absoluteY="205dp" />

        <Button
            android:id="@+id/button3"
            android:layout_width="180dp"
            android:layout_height="180dp"
            android:layout_marginStart="21dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="16dp"
            android:backgroundTint="#e7b3b3"
            android:text="+"
            android:textColorLink="#E7F85AB4"
            android:textSize="34dp"
            app:iconPadding="0dp"
            app:layout_constraintBottom_toTopOf="@+id/button7"
            app:layout_constraintEnd_toStartOf="@+id/button5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextNumberDecimal2" />

        <Button
            android:id="@+id/button5"
            android:layout_width="180dp"
            android:layout_height="180dp"
            android:backgroundTint="#575151"
            android:text="-"
            android:textSize="34dp"
            tools:layout_editor_absoluteX="215dp"
            tools:layout_editor_absoluteY="282dp" />

        <Button
            android:id="@+id/button6"
            android:layout_width="180dp"
            android:layout_height="180dp"
            android:backgroundTint="#feb062"
            android:text="/"
            android:textSize="34dp"
            tools:layout_editor_absoluteX="215dp"
            tools:layout_editor_absoluteY="478dp" />

        <Button
            android:id="@+id/button7"
            android:layout_width="180dp"
            android:layout_height="180dp"
            android:layout_marginTop="16dp"
            android:backgroundTint="#575151"
            android:text="x"
            android:textSize="34dp"
            app:layout_constraintTop_toBottomOf="@+id/button3"
            tools:layout_editor_absoluteX="21dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="24dp"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="30dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="102dp"
        android:layout_height="35dp"
        android:text="TextView"
        android:textSize="24dp"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="155dp" />

    <EditText
        android:id="@+id/editTextNumberDecimal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="numberDecimal"
        android:scrollbarSize="34dp"
        android:text="23"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="78dp" />

    <EditText
        android:id="@+id/editTextNumberDecimal2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="numberDecimal"
        android:scrollbarSize="34dp"
        android:text="12"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="205dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#e7b3b3"
        android:text="+"
        android:textColorLink="#E7F85AB4"
        android:textSize="34dp"
        app:iconPadding="0dp"
        app:layout_constraintBottom_toTopOf="@+id/button7"
        app:layout_constraintEnd_toStartOf="@+id/button5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextNumberDecimal2" />

    <Button
        android:id="@+id/button5"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:backgroundTint="#575151"
        android:text="-"
        android:textSize="34dp"
        tools:layout_editor_absoluteX="215dp"
        tools:layout_editor_absoluteY="282dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:backgroundTint="#feb062"
        android:text="/"
        android:textSize="34dp"
        tools:layout_editor_absoluteX="215dp"
        tools:layout_editor_absoluteY="478dp" />

    <Button
        android:id="@+id/button7"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="#575151"
        android:text="x"
        android:textSize="34dp"
        app:layout_constraintTop_toBottomOf="@+id/button3"
        tools:layout_editor_absoluteX="21dp" />

</androidx.constraintlayout.widget.ConstraintLayout>